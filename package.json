{
  "name": "Discourse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-ios": "cd ios && xcodebuild -scheme Discourse -destination 'platform=iOS Simulator,name=iPhone X' clean",
    "prettier": "prettier --write \"js/**/*.js\"",
    "lint": "eslint ./js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17",
    "moment": "^2.22",
    "prop-types": "^15.6.0",
    "react": "16.4.1",
    "react-native": "0.56.1",
    "react-native-device-info": "^0.22",
    "react-native-key-pair": "git+https://github.com/samsaffron/react-native-key-pair.git",
    "react-native-orientation": "git+https://github.com/yamill/react-native-orientation.git",
    "react-native-progress": "^3.5",
    "react-native-sortable-listview": "github:deanmcpherson/react-native-sortable-listview",
    "react-native-swipeout": "^2.3",
    "react-native-vector-icons": "^5.0",
    "react-navigation": "^1.5",
    "invariant": "^2.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-react-native": "^5",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.14"
  }
}
