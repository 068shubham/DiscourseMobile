{
  "name": "DiscourseMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 9001",
    "storybook-ios": "storybook run-ios -a DiscourseMobileStorybook -i org.discourse.DiscourseMobileStorybook",
    "clean-ios": "cd ios && xcodebuild -scheme DiscourseMobile -destination 'platform=iOS Simulator,name=iPhone 6' clean"
  },
  "dependencies": {
    "keypair": "^1.0.0",
    "lodash": "^4.15.0",
    "react": "15.2.1",
    "react-native": "^0.31",
    "react-native-background-fetch": "^0.1.0",
    "react-native-device-info": "^0.9.4",
    "react-native-ecc": "^1.2.2",
    "react-native-randombytes": "^1.0.1",
    "react-native-safari-view": "^1.0.1",
    "react-native-swipeout": "git+https://github.com/dancormier/react-native-swipeout.git",
    "sjcl": "^1.0.3",
    "react-native-progress": "^3.0.1"
  },
  "devDependencies": {
    "@kadira/react-native-storybook": "^1.12.3"
  }
}
