{
  "name": "Discourse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 9001",
    "storybook-ios": "storybook run-ios -a DiscourseStorybook -i org.discourse.DiscourseStorybook",
    "clean-ios": "cd ios && xcodebuild -scheme Discourse -destination 'platform=iOS Simulator,name=iPhone 6' clean",
    "lint": "eslint ./js"
  },
  "dependencies": {
    "keypair": "^1.0.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "react": "15.3.1",
    "react-native": "0.32.0",
    "react-native-background-fetch": "git+https://github.com/samsaffron/react-native-background-fetch.git",
    "react-native-device-info": "^0.9.4",
    "react-native-ecc": "^1.2.2",
    "react-native-fetch-blob": "^0.9.3",
    "react-native-progress": "^3.0.1",
    "react-native-randombytes": "^1.0.1",
    "react-native-safari-view": "git+https://github.com/samsaffron/react-native-safari-view.git",
    "react-native-swipeout": "git+https://github.com/dancormier/react-native-swipeout.git",
    "sjcl": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-react-native": "^1.9.0",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.1",
    "@kadira/react-native-storybook": "^1.12.6"
  }
}
