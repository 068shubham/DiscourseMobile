{
  "name": "Discourse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean-ios": "cd ios && xcodebuild -scheme Discourse -destination 'platform=iOS Simulator,name=iPhone 6' clean",
    "prettier": "prettier --write \"js/**/*.js\"",
    "lint": "eslint .",
    "test": "jest",
    "build-rn-webview": "cd node_modules/react-native-webview/ && yarn install && yarn tsc && rm -rf node_modules/react-native-webview/node_modules/",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/push-notification-ios": "^1.0.3",
    "immutable": "^4.0.0-rc.12",
    "invariant": "^2.2",
    "jetifier": "^1.6.3",
    "lodash": "^4.17",
    "moment": "^2.22",
    "prop-types": "^15.6.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-device-info": "^3.1.4",
    "react-native-fabric": "^0.5.2",
    "react-native-firebase": "^5.4.2",
    "react-native-immutable-list-view": "^2.0.0",
    "react-native-key-pair": "https://github.com/samsaffron/react-native-key-pair",
    "react-native-orientation": "git+https://github.com/yamill/react-native-orientation.git",
    "react-native-progress": "^4.0.3",
    "react-native-safari-view": "git+https://github.com/samsaffron/react-native-safari-view.git",
    "react-native-safari-web-auth": "1.0.3",
    "react-native-sortable-listview": "0.2.9",
    "react-native-swipe-list-view": "^2.1.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "7.6.0",
    "react-navigation": "^1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.14",
    "react-test-renderer": "16.9.0"
  }
}
